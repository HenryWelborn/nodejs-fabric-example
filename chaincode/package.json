{
    "name": "fabcar",
    "version": "1.0.0",
    "description": "FabCar contract implemented in TypeScript",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "pretest": "npm run lint",
        "test": "nyc mocha -r ts-node/register src/**/*.spec.ts",
        "start": "fabric-chaincode-node start",
        "build": "tsc",
        "build:watch": "tsc -w",
        "prepublishOnly": "npm run build",
        "nvm": "~/.nvm/nvm-exec which node | xargs -J % ln -sf % ./.nvm-node && chmod +x ./.nvm-node"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "fabric-contract-api": "^1.4.5",
        "fabric-shim": "^2.2.3"
    },
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.17.18",
        "@types/sinon": "^5.0.7",
        "@types/sinon-chai": "^3.2.4",
        "chai": "^4.2.0",
        "mocha": "^6.2.3",
        "nyc": "^15.0.1",
        "sinon": "^7.1.1",
        "sinon-chai": "^3.5.0",
        "ts-node": "^7.0.1",
        "tslint": "^5.20.1",
        "typescript": "^3.8.3"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "coverage/**",
            "dist/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
